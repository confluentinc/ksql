    {
      "name": "earliest by offset with arrays, structs, and maps",
      "statements": [
        "CREATE STREAM INPUT (ID BIGINT KEY, F0 ARRAY<STRUCT<A VARCHAR, M MAP<STRING, DOUBLE>>>) WITH (kafka_topic='test_topic', value_format='JSON');",
        "CREATE TABLE OUTPUT AS SELECT ID, EARLIEST_BY_OFFSET(F0) AS L0 FROM INPUT GROUP BY ID;"
      ],
      "inputs": [
        {"topic": "test_topic", "key": 0, "value": {"F0": [{"A": "Early0", "M": {"Early0": 1.234}}, {"A": "Early2", "M": {"Early2": 1.23456}}]}},
        {"topic": "test_topic", "key": 1, "value": {"F0": [{"A": "Early1", "M": {"Early0": 2.345}}]}},
